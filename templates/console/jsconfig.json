{
  "compilerOptions": {
    "experimentalDecorators": true,
    "jsx": "react",
    "target": "ES6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "Assets/*": ["./src/assets/*"],
      "Common/*": ["./src/common/*"],
      "Components/*": ["./src/components/*"],
      "Constants/*": ["./src/constants/*"],
      "Layouts/*": ["./src/layouts/*"],
      "Models/*": ["./src/models/*"],
      "Pages/*": ["./src/pages/*"],
      "Store/*": ["./src/store/*"],
      "Styles/*": ["./src/styles/*"],
      "Utils/*": ["./src/utils/*"],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}